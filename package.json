{
  "name": "api-lab",
  "version": "3.0.2",
  "description": "Node.js sample app suitable for cloud containers",
  "main": "server.js",
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/geoip-lite": "^1.1.31",
    "@types/morgan": "^1.9.0",
