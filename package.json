    "prettier": "^2.2.1",
    "ts-jest": "^27.0.1"
  },
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build-ts": "tsc",
    "clear-dist-dir": "rm -rf dist/[^.]*",
